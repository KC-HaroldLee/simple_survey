{
    "name": "page1",
    "elements": [
     {
      "type": "panel",
      "name": "issue-report",
      "elements": [
       {
        "type": "text",
        "name": "issue-title",
        "width": "100%",
        "minWidth": "auto",
        "title": "별명을 입력해주세요"
       },
       {
        "type": "comment",
        "name": "issue-details",
        "width": "100%",
        "minWidth": "auto",
        "title": "Details",
        "maxLength": 512,
        "rows": 6,
        "placeholder": "Introduce the problem and expand on what you put in the title. Describe what you tried, what you expected to happen, and what actually resulted.",
        "autoGrow": true,
        "allowResize": false
       },
       {
        "type": "rating",
        "name": "issue-priority",
        "title": "Level of priority",
        "autoGenerate": false,
        "rateCount": 3,
        "rateValues": [
         {
          "value": 1,
          "text": "Low"
         },
         {
          "value": 2,
          "text": "Medium"
         },
         {
          "value": 3,
          "text": "High"
         }
        ]
       },
       {
        "type": "file",
        "name": "attached-files",
        "title": "Attachments",
        "allowMultiple": true,
        "filePlaceholder": "Click the button below to select files to upload."
       },
       {
        "type": "text",
        "name": "email",
        "width": "100%",
        "minWidth": "auto",
        "title": "Email address"
       }
      ],
      "width": "100%"
     }
    ]
   }